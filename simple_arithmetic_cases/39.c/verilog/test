#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x5638d69464d0 .scope module, "smaller_tb" "smaller_tb" 2 1;
 .timescale 0 0;
v0x5638d69929f0_0 .net "c", 9 0, v0x5638d6958c80_0;  1 drivers
v0x5638d6992ad0_0 .var "clk", 0 0;
v0x5638d6992ba0_0 .var/i "i", 31 0;
v0x5638d6992c70_0 .net "n", 9 0, v0x5638d6992690_0;  1 drivers
v0x5638d6992d40_0 .var "rst", 0 0;
v0x5638d6992e30_0 .var "selector", 0 0;
S_0x5638d6946650 .scope module, "RTL" "top" 2 9, 3 1 0, S_0x5638d69464d0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "selector"
    .port_info 1 /INPUT 1 "clk"
    .port_info 2 /INPUT 1 "rst"
    .port_info 3 /OUTPUT 10 "c"
    .port_info 4 /OUTPUT 10 "n"
v0x5638d6958c80_0 .var "c", 9 0;
v0x5638d69925d0_0 .net "clk", 0 0, v0x5638d6992ad0_0;  1 drivers
v0x5638d6992690_0 .var "n", 9 0;
v0x5638d6992780_0 .net "rst", 0 0, v0x5638d6992d40_0;  1 drivers
v0x5638d6992840_0 .net "selector", 0 0, v0x5638d6992e30_0;  1 drivers
E_0x5638d697fb10 .event posedge, v0x5638d69925d0_0;
    .scope S_0x5638d6946650;
T_0 ;
    %wait E_0x5638d697fb10;
    %vpi_call 3 10 "$display", "c: %b, n: %b", v0x5638d6958c80_0, v0x5638d6992690_0 {0 0 0};
    %jmp T_0;
    .thread T_0;
    .scope S_0x5638d6946650;
T_1 ;
    %wait E_0x5638d697fb10;
    %load/vec4 v0x5638d6992780_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 10;
    %assign/vec4 v0x5638d6958c80_0, 0;
    %pushi/vec4 500, 0, 10;
    %assign/vec4 v0x5638d6992690_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x5638d6992840_0;
    %load/vec4 v0x5638d6958c80_0;
    %load/vec4 v0x5638d6992690_0;
    %cmp/e;
    %flag_get/vec4 4;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.2, 8;
    %pushi/vec4 1, 0, 10;
    %assign/vec4 v0x5638d6958c80_0, 0;
    %load/vec4 v0x5638d6992690_0;
    %assign/vec4 v0x5638d6992690_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v0x5638d6992840_0;
    %load/vec4 v0x5638d6958c80_0;
    %load/vec4 v0x5638d6992690_0;
    %cmp/e;
    %flag_get/vec4 4;
    %inv;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.4, 8;
    %load/vec4 v0x5638d6958c80_0;
    %addi 1, 0, 10;
    %assign/vec4 v0x5638d6958c80_0, 0;
    %load/vec4 v0x5638d6992690_0;
    %assign/vec4 v0x5638d6992690_0, 0;
    %jmp T_1.5;
T_1.4 ;
    %load/vec4 v0x5638d6992690_0;
    %assign/vec4 v0x5638d6992690_0, 0;
    %load/vec4 v0x5638d6958c80_0;
    %assign/vec4 v0x5638d6958c80_0, 0;
T_1.5 ;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5638d69464d0;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5638d6992ad0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5638d6992d40_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x5638d69464d0;
T_3 ;
    %delay 5, 0;
    %load/vec4 v0x5638d6992ad0_0;
    %inv;
    %store/vec4 v0x5638d6992ad0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x5638d69464d0;
T_4 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5638d6992ba0_0, 0, 32;
T_4.0 ;
    %load/vec4 v0x5638d6992ba0_0;
    %cmpi/s 1000, 0, 32;
    %jmp/0xz T_4.1, 5;
    %vpi_func 2 37 "$random" 32 {0 0 0};
    %pad/u 1;
    %assign/vec4 v0x5638d6992e30_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x5638d6992d40_0, 0;
    %delay 10, 0;
    %load/vec4 v0x5638d6992ba0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5638d6992ba0_0, 0, 32;
    %jmp T_4.0;
T_4.1 ;
    %vpi_call 2 44 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
